{
  "name": "components",
  "version": "0.1.7",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:storybook": "BROWSER=none start-storybook -p 6006",
    "build": "npm run build:lib; npm run build:storybook",
    "build:lib": "vue-cli-service build --target lib --name ctlib ./src/index.js",
    "build:storybook": "build-storybook -o ./docs",
    "lint": "vue-cli-service lint",
    "preversion": "npm run lint",
    "version": "npm run build && git add .",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vuetify": "^2.4.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.6.3",
    "@mdi/font": "^4.5.95",
    "material-design-icons-iconfont": "^6.1.0",
    "@storybook/addon-a11y": "^6.2",
    "@storybook/addon-actions": "^6.2",
    "@storybook/addon-knobs": "^6.2",
    "@storybook/addon-notes": "^5.3",
    "@storybook/addon-viewport": "^6.2",
    "@storybook/addons": "^6.2",
    "@storybook/vue": "^6.2",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "js-beautify": "^1.11.0",
    "prism-react-renderer": "^0.1.7",
    "prismjs": "^1.17.1",
    "sass": "^1.32.0",
    "sass-loader": "^10.0.0",
    "vue-cli-plugin-vuetify": "~2.3.1",
    "vue-cli-plugin-vuetify-storybook": "~0.2.1",
    "vue-storybook": "^1.2.0",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  },
  "peerDependencies": {
    "sass-loader": "^10.0.0",
    "vue": "^2.6.11",
    "vuetify": "^2.4.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "main": "dist/ctlib.common.js",
  "_main": "src/index.js"
}
